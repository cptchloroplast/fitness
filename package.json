{
  "name": "fitness",
  "version": "0.1.0",
  "description": "Wahoo to Garmin Sync",
  "main": "src/cloudflare/index.ts",
  "scripts": {
    "build": "npm-run-all build:worker build:functions",
    "build:functions":"zip -j dist/okkema-fitness.zip requirements.txt src/google/*.py",
    "build:worker": "wrangler deploy src/cloudflare/index.ts --dry-run --outdir dist",
    "start": "wrangler dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "okkema",
    "fitness",
    "wahoo",
    "garmin"
  ],
  "author": "Ben Okkema <ben@okkema.org>",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250601.0",
    "@okkema/worker": "^3.10.0",
    "@types/mime-types": "^3.0.0",
    "mime-types": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postal-mime": "^2.4.3",
    "wrangler": "^4.14.1"
  }
}
